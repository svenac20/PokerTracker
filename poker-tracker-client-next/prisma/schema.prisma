generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model usercasinos {
  my_row_id BigInt @id @db.UnsignedBigInt
  A         Int
  B         String

  @@unique([A, B], map: "_UserCasinos_AB_unique")
  @@index([B], map: "_UserCasinos_B_index")
  @@map("_usercasinos")
}

model casino {
  id     Int    @id @default(autoincrement())
  name   String
  townId Int

  @@index([townId], map: "Casino_townId_fkey")
}

model country {
  id   Int    @id @default(autoincrement())
  name String
}

model gametype {
  id   Int    @id @default(autoincrement())
  name String
}

model pokergame {
  id            Int      @id @default(autoincrement())
  limit         String
  gameTypeId    Int
  playerWaiting Int
  tablesNumber  Int
  casinoId      Int
  startTime     DateTime @default(now())

  @@index([casinoId], map: "PokerGame_casinoId_fkey")
  @@index([gameTypeId], map: "PokerGame_gameTypeId_fkey")
}

model role {
  id   Int    @id @default(autoincrement())
  name String
}

model town {
  id        Int    @id @default(autoincrement())
  name      String
  countryId Int

  @@index([countryId], map: "Town_countryId_fkey")
}

model user {
  id       String  @id
  email    String  @unique(map: "User_email_key")
  googleId String? @unique(map: "User_googleId_key")
  username String
  roleId   Int

  @@index([roleId], map: "User_roleId_fkey")
}